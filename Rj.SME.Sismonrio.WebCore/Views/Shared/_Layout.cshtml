@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Rj.SME.Sismonrio.WebCore</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    @*<link rel="stylesheet" href="~/css/site.css" />*@
    <link rel="stylesheet" href="~/css/geral.css" />

    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>



    <!-- CABEÇALHO, LOGO, TÍTULO SISTEMA-->
    <div class="header">
        <div class="logoCab clickVoltaPaginaInicial"></div>
        <div class="logoTituloCab hidden-xs clickVoltaPaginaInicial">
            <label class="clickVoltaPaginaInicial">SME - Sistema de Monitoramento para Educação Infantil </label>
        </div>
        <div class="barraUsuario">
            <label class="lbl_usr" data-original-title="Usuário do Sistema">Gabriel Oliveira</label>
            <label class="lbl_funcao" data-original-title="Usuário do Sistema"> Desenvolvedor .NET / Porto Alegre - RS</label>
        </div>
    </div>

    <nav class="navbar navbar-toggleable-md navbar-light bg-faded" style="    background-color: #0F61A4;
    border: solid 1px black;">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" href="#" style="color: #fff;">Home <span class="sr-only">(current)</span></a>
                </li> 
                <li class="nav-item">
                    <a class="nav-link" href="#" style="color: #fff;">Cadastros</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" style="color: #fff;">Formulários</a>
                </li>
            </ul>
        </div>
    </nav>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/tether/dist/js/tether.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

    @RenderSection("Scripts", required: false)
    <!-- Polyfill(s) for older browsers -->
    <script src="~/lib/node/shim.min.js"></script>
    <script src="~/lib/node/zone.js"></script>
    <script src="~/lib/node/system.src.js"></script>
    <script src="~/App/systemjs.config.js"></script>
    <script>
        System.import('/App/main').catch(function (err) {
            console.error(err);
        });
    </script>

   

    <!-- CAMINHO DE PÃO -->
